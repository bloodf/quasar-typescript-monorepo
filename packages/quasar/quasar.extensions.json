{
  "@quasar/typescript": {
    "webpack": "plugin",
    "rename": false,
    "vscode": false,
    "prettier": false
  },
  "@quasar/testing": {
    "harnesses": [
      "unit-jest",
      "e2e-cypress"
    ]
  },
  "@quasar/testing-unit-jest": {
    "babel": "babelrc",
    "options": [
      "scripts",
      "SFC",
      "wallabyjs",
      "majestic"
    ]
  },
  "@quasar/testing-e2e-cypress": {
    "options": [
      "scripts"
    ]
  },
  "@quasar/dotenv": {
    "env_development": ".env.dev",
    "env_production": ".env",
    "common_root_object": "none",
    "create_env_files": true,
    "add_env_to_gitignore": false
  }
}