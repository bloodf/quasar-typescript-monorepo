{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "../../packages/quasar/",
    "paths": {
      "app/*": [
        "./*"
      ],
      "src/*": [
        "./src/*"
      ],
      "components/*": [
        "./src/components/*"
      ],
      "layouts/*": [
        "./src/layouts/*"
      ],
      "pages/*": [
        "./src/pages/*"
      ],
      "assets/*": [
        "./src/assets/*"
      ],
      "boot/*": [
        "./src/boot/*"
      ],
      "store/*": [
        "./src/store/*"
      ],
      "utils/*": [
        "./src/utils/*"
      ],
      "cypress/*": [
        "./test/cypress/*"
      ],
      "jest/*": [
        "./test/jest/*"
      ],
      "jest/tests/*": [
        "./test/jest/__tests__/*"
      ],
      "loaders/*": [
        "./test/loaders/*"
      ]
    }
  },
  "include": [
    "."
  ]
}
