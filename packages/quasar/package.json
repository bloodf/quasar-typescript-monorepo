{
  "name": "@quasar-monorepo/quasar",
  "version": "0.0.1",
  "description": "A Quasar Framework app",
  "author": "Heitor Ramon Ribeiro <heitor.ramon@gmail.com>",
  "license": "MIT",
  "scripts": {
    "bootstrap": "lerna bootstrap --use-workspaces",
    "build": "quasar build",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\"",
    "lint": "npm run lint:src && npm run lint:test",
    "lint:fix": "npm run lint:fix:src && npm run lint:fix:test",
    "lint:fix:src": "eslint --fix --ext .js,.vue,.ts,.tsx src",
    "lint:fix:test": "eslint --fix --ext .js,.vue,.ts,.tsx test",
    "lint:src": "eslint --ext .js,.vue,.ts,.tsx src",
    "lint:test": "eslint --ext .js,.vue,.ts,.tsx test",
    "precommit": "lint-staged",
    "serve": "quasar dev",
    "serve:test:coverage": "quasar serve tests/jest/coverage/lcov-report/ --port 8788",
    "styleguide": "vue-styleguidist server",
    "styleguide:build": "vue-styleguidist build",
    "test": "jest --updateSnapshot",
    "test:coverage": "jest --coverage",
    "test:ui": "majestic",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll",
    "update:check": "ncu",
    "update:update": "ncu -u"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ],
  "dependencies": {
    "@quasar/extras": "^1.9.4",
    "@vue/composition-api": "^0.6.7",
    "core-js": "^3.6.5",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "quasar": "^1.12.13",
    "vue": "^2.6.11",
    "vuelidate": "^0.7.5",
    "vue-class-component": "^7.2.5",
    "vue-i18n": "^8.21.0",
    "vue-property-decorator": "^9.0.0",
    "vue-apollo": "^3.0.4",
    "vue-apollo-decorators": "^2.0.0",
    "vue-router": "^3.4.3",
    "vuex": "^3.5.1",
    "vuex-class": "^0.3.2"
  },
  "devDependencies": {
    "jest": "^26.4.0",
    "majestic": "^1.7.0",
    "dotenv-webpack": "^3.0.0",
    "@types/vuelidate": "^0.7.13",
    "@quasar/app": "^2.0.6",
    "@quasar/cli": "^1.1.0",
    "@quasar/quasar-app-extension-dotenv": "^1.0.5",
    "@quasar/quasar-app-extension-testing": "^1.0.0",
    "@quasar/quasar-app-extension-testing-unit-jest": "^1.0.1",
    "@testing-library/jest-dom": "^5.11.3",
    "@vue/test-utils": "^1.0.4",
    "vue-styleguidist": "^4.31.0",
    "workbox-webpack-plugin": "^5.1.3"
  },
  "engines": {
    "node": ">= 12",
    "npm": ">= 6.13.4",
    "yarn": ">= 1"
  },
  "capacitorId": "",
  "cordovaId": "org.cordova.quasar.app",
  "shortProductName": "Web App",
  "productName": "Quasar App"
}
